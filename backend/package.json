   "devDependencies": {
      "@types/node": "^12.7.1",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/bluebird": "^3.5.27",
      "@types/dotenv": "^6.1.1",
      "@types/express": "^4.17.0",
      "@types/jsonwebtoken": "^8.3.2",
      "@types/markdown-it": "0.0.8",
      "@types/md5": "^2.1.33",
      "@types/morgan": "^1.7.36",
      "@types/nanoid": "^2.0.0",
      "@types/nodemailer": "^6.2.1",
      "@types/nodemailer-smtp-transport": "^2.7.4",
      "@types/validator": "^10.11.2",
      "@types/ws": "^6.0.2",
      "argon2": "^0.24.0",
      "bluebird": "^3.5.5",
      "body-parser": "^1.18.1",
      "cookie-parser": "^1.4.4",
      "dotenv": "^8.0.0",
      "es6-promise": "^4.2.8",
      "express": "^4.15.4",
      "jsonwebtoken": "^8.5.1",
      "katex": "^0.10.2",
      "md5": "^2.2.1",
      "morgan": "^1.9.1",
      "nanoid": "^2.1.0",
      "nodemailer": "^6.3.0",
      "reflect-metadata": "^0.1.10",
      "slugify": "^1.3.5",
      "sqlite3": "^4.0.3",
      "ts-node-dev": "^1.0.0-pre.40",
      "typeorm": "^0.2.18",
      "validator": "^11.1.0",
      "ws": "^7.1.2"
   },
   "scripts": {
      "start": "ts-node src/app.ts",
      "tsc": "tsc -p ./",
      "dev": "ts-node-dev --respawn --transpileOnly src/app.ts",
      "prod": "tsc -p ./ && node ./build/app.js",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js",
      "migration-create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
      "migrate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
      "run-migrate": "ts-node ./node_modules/typeorm/cli.js migration:run"
   }
}
