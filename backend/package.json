{
   "name": "new-typeorm-project",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "ts-node": "3.3.0",
      "@types/node": "^8.0.29",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/express": "^4.17.0",
      "@types/jsonwebtoken": "^8.3.2",
      "@types/markdown-it": "0.0.8",
      "argon2": "^0.24.0",
      "body-parser": "^1.18.1",
      "cookie-parser": "^1.4.4",
      "express": "^4.15.4",
      "jsonwebtoken": "^8.5.1",
      "katex": "^0.10.2",
      "reflect-metadata": "^0.1.10",
      "sqlite3": "^4.0.3",
      "ts-node-dev": "^1.0.0-pre.40",
      "typeorm": "^0.2.18"
   },
   "scripts": {
      "start": "ts-node src/app.ts",
      "tsc": "tsc -p ./",
      "dev": "ts-node-dev --respawn --transpileOnly src/app.ts",
      "prod": "tsc -p ./ && node ./build/app.js",
      "typeorm": "ts-node ./node_modules/.bin/typeorm",
      "migration-create": "ts-node ./node_modules/.bin/typeorm migration:create -n",
      "migrate": "ts-node ./node_modules/.bin/typeorm migration:generate -n",
      "run-migrate": "ts-node ./node_modules/.bin/typeorm migration:run"
   }
}
